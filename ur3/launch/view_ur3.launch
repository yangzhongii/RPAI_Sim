<?xml version="1.0"?>
<launch>
  <include file="$(find ur3)/launch/load_ur3.launch"/>

  <!-- <node name="joint_state_publisher" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" /> -->
  <node pkg="ur3" type="ur3_virtual_controller.py" name='ur3_virtual_controller' output="screen" />
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
  <rosparam param="source_list">[/ur3_probe_joint_states] </rosparam>
  <!-- <rosparam param="source_list">[/joint_commands] </rosparam>   -->
    <param name="rate" value="30"/>
  </node>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ur3)/cfg/view_robot.rviz" required="true" />
</launch>
